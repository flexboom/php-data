{
    "name": "flexboom/php-data",
    "type": "library",
    "license": "MIT",
    "description": "Getting data from e.g. a API response and convert it to strongly typed data can be a hassle, this package addresses that.",
    "autoload": {
        "psr-4": {
            "Flexboom\\PhpData\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flexboom\\PhpData\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^12.2.7",
        "phpstan/phpstan": "^2.1.19",
        "friendsofphp/php-cs-fixer": "^3.84.0"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests",
        "code-style": "./vendor/bin/php-cs-fixer fix src --dry-run --diff",
        "static-analytic": "./vendor/bin/phpstan"
    }
}
