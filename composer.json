{
    "name": "flexboom/php-data",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flexboom\\PhpData\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flexboom\\PhpData\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^12.3@dev",
        "phpstan/phpstan": "2.1.x-dev",
        "friendsofphp/php-cs-fixer": "dev-master"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests",
        "code-style": "./vendor/bin/php-cs-fixer fix src --dry-run --diff"
        "static-analytic": "./vendor/bin/phpstan"
    }
}
